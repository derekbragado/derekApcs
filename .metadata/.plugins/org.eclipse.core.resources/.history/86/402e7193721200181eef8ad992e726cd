public class Average
{
	//instance variables
	private double one, two, average;

	public void setNums(double num1, double num2)
	{
		num1 = one;
		num2 = two;


	}

	public void average( )
	{
		double sum=0;
		average = sum;
		average = (one*two)/2;

	}
	public void print( )
	{
		System.out.println(average);



	}
}